cp server-adminlist.json ~
cp saves/ali.zip ~
cp data/server-settings.json ~
cd ..
rm -rf factorio
cd ~
rm linux64
wget https://factorio.com/get-download/latest/headless/linux64
tar -xvf linux64
mv factorio /opt/
cd /opt/factorio
mv ~/server-settings.json data/
mv ~/server-adminlist.json .
mkdir saves
mv ~/ali.zip saves/
./bin/x64/factorio --use-server-whitelist --server-whitelist ./data/server-whitelist.json --server-settings ./data/server-settings.json --start-server ./saves/keke.zip
